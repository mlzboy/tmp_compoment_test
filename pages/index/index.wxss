.weui-navbar{
  margin:0;
  padding:0;
   width: 100%;
   /*
   position: fixed;
   
   border-bottom: 1px solid #00c355;
   */
   box-sizing: border-box;
   white-space: nowrap; 
   z-index: 100;
   background: white;
}

.weui-navbar .default-item{
  /*width:25%;*/
  display: inline-block;
  text-align: center;
  font-size: 36rpx;
  box-sizing: border-box;
  height: 7%;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  width:240rpx;
}
.weui-navbar .item_on{
   /*color: #00c355;*/
}
.weui-navbar-slider{
    position: absolute;
    border-radius: 12rpx;
    width: 30rpx;
    height: 12rpx;
    content:"";
    /*background: #00c355;*/
    bottom: 0px;
    left: 0rpx;
    transition: transform .3s;
}


.footer
{
  margin: 0;
  padding:0;
  //position: fixed;
  top: -600rpx;
  width: 100%;
  background:blue;
  
  transform: translateY(0%);
  transition: all 0.4s ease;
  
  z-index:12;
}

