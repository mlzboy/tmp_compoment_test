page{margin:0;padding:0}
::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}
.hidden
{
  display:none;
}
.on
{
  display:block;
}

.memory_header
{
margin:40rpx 0 40rpx 40rpx;
display:flex;
flex-direction:row;
align-items:center;
}

.memory_icon {
border-radius: 15rpx;
width: 12rpx;
height: 30rpx; 
font-size: 30rpx;
text-align: center;
line-height: 30rpx;
  background: #00c355;


}
.memory_title
{

margin-left: 24rpx;
font-size: 36rpx;
line-height: 36rpx;
height:36rpx;
font-weight: normal;
word-break: break-all;
}



.memory_content
{
  margin: 30rpx 0;
  display: flex;
  justify-content: center;
}
.clear
{
clear:both;
height:0;
}






.block_test
{
  display:inline-block;
}
.text_test
{
  text-align:center;
  color:gray;
  font-size: 24rpx;
  margin:5rpx 40rpx 15px 40rpx;
}

.sv
{
  width:90%;
 display:flex; justify-content:center; align-items:center;
     overflow: hidden;
    white-space: nowrap;
    height: 350rpx;
    margin:0 auto;

}
.sv2 
{
   width: 90%;
    display:flex; justify-content:center; align-items:center;
    overflow: hidden;
