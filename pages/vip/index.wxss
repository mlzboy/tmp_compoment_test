.btn{
    margin-right:20rpx;
    line-height: 80rpx;
    height: 80rpx;
    color:#00c355;
}

.content{
    line-height: 80rpx;
    height: 80rpx;
}
.sv{
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    margin:0;
    padding:0;
    width:100%;
    height:100%;
}

.table{
    margin:0 auto;
    padding:0;
    width:100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;

}
.l{

    border-left-width: 1rpx;
    border-left-style: solid;
    border-left-color:gainsboro;
}
.t{

    border-top-width: 1rpx;
    border-top-style: solid;
    border-top-color: gainsboro;

}
.cell{
    margin:0;
    padding:0;
    width:200rpx;
    height:60rpx;
    line-height:60rpx;
    border-right-style: solid;
    border-right-color:gainsboro;
    border-right-width: 1rpx;

    border-bottom-style: solid;
    border-bottom-color:gainsboro;
    border-bottom-width: 1rpx;
    text-align: center;
}
.cell2{
    margin:0;
    padding:0;
    width:200rpx;
    height:60rpx;
    line-height:60rpx;
    border-right-width: 1rpx;
    border-right-style: solid;
    border-right-color:gainsboro;
    border-bottom-width: 1rpx;
    border-bottom-style: solid;
    border-bottom-color: gainsboro;
    text-align: center;
}

