
.row{
    width:100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.title{
    width:160rpx;
    margin-left:20rpx;
    line-height:80rpx;
    height: 80rpx;
}

input{
   height: 80rpx;
   line-height: 80rpx; 
}

.title2{
    font-size: 24rpx;
    margin:-20rpx 0 0 20rpx;
    line-height:80rpx;
    height: 80rpx;
}

.bottom{
    border-bottom-color: gainsboro;
    border-bottom-width: 1rpx;
    border-bottom-style: solid;
}

